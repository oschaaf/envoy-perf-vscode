{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include",
                "/usr/local/include",
                "${workspaceFolder}/**",
                "${workspaceFolder}/nighthawk/**",
                "${workspaceFolder}/bazel-nighthawk/**"
            ],
            "defines": [],
            "compilerPath": "/usr/lib/llvm-8/bin/clang++",
            "compileCommands": "${workspaceFolder}/nighthawk/compile_commands.json",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "/usr/include",
                    "/usr/local/include",
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/nighthawk/**",
                    "${workspaceFolder}/bazel-nighthawk/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}